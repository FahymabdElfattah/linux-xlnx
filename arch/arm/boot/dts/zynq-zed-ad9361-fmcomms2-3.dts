			reg = <0xf8007100 0x20>;
			interrupts = <0x00 0x07 0x04>;
			interrupt-parent = <0x01>;
			clocks = <0x02 0x0c>;
		};

		can@e0008000 {
			compatible = "xlnx,zynq-can-1.0";
